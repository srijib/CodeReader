<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Drawer Labels -->
    <string-array name="label_items_my">
        <item>@string/item_wallet</item>
        <item>@string/item_type</item>
    </string-array>

    <string-array name="label_items_money">
        <item>@string/item_transactions</item>
        <item>@string/item_income</item>
        <item>@string/item_expense</item>
    </string-array>

    <string-array name="label_items_chart">
        <item>@string/item_chart_day</item>
        <item>@string/item_chart_week</item>
        <item>@string/item_chart_month</item>
        <item>@string/item_chart_year</item>
    </string-array>

    <!--####-->

    <!--Drawer Icons-->
    <integer-array name="icon_items_my">
        <item>@drawable/ic_drawer_my_wallet</item>
        <item>@drawable/ic_drawer_transaction_types</item>
    </integer-array>
    <integer-array name="icon_items_money">
        <item>@drawable/ic_drawer_transactions</item>
        <item>@drawable/ic_drawer_income</item>
        <item>@drawable/ic_drawer_expense</item>
    </integer-array>
    <integer-array name="icon_items_chart">
        <item>@drawable/ic_drawer_cart</item>
        <item>@drawable/ic_drawer_cart</item>
        <item>@drawable/ic_drawer_cart</item>
        <item>@drawable/ic_drawer_cart</item>
    </integer-array>
    <!--#########-->

    <!-- colorPrimary -->
    <integer-array name="color_my_primary">
        <item>@color/wallet_primary</item>
        <item>@color/type_primary</item>
    </integer-array>
    <integer-array name="color_money_primary">
        <item>@color/transaction_primary</item>
        <item>@color/income_primary</item>
        <item>@color/expense_primary</item>
    </integer-array>
    <integer-array name="color_chart_primary">
        <item>@color/chart_primary</item>
        <item>@color/chart_primary</item>
        <item>@color/chart_primary</item>
        <item>@color/chart_primary</item>
    </integer-array>
    <!-- ###############-->
    <!--colorPrimaryDark-->
    <integer-array name="color_my_primary_dark">
        <item>@color/wallet_primary_dark</item>
        <item>@color/type_primary_dark</item>
    </integer-array>
    <integer-array name="color_money_primary_dark">
        <item>@color/transaction_primary_dark</item>
        <item>@color/income_primary_dark</item>
        <item>@color/expense_primary_dark</item>
    </integer-array>
    <integer-array name="color_chart_primary_dark">
        <item>@color/chart_primary_dark</item>
        <item>@color/chart_primary_dark</item>
        <item>@color/chart_primary_dark</item>
        <item>@color/chart_primary_dark</item>
    </integer-array>
    <!--##################-->
    <integer-array name="drawer_labels">
        <item>@array/label_items_my</item>
        <item>@array/label_items_money</item>
        <item>@array/label_items_chart</item>
    </integer-array>
    <integer-array name="drawer_icons">
        <item>@array/icon_items_my</item>
        <item>@array/icon_items_money</item>
        <item>@array/icon_items_chart</item>
    </integer-array>
    <integer-array name="drawer_color_primary">
        <item>@array/color_my_primary</item>
        <item>@array/color_money_primary</item>
        <item>@array/color_chart_primary</item>
    </integer-array>
    <integer-array name="drawer_color_primary_dark">
        <item>@array/color_my_primary_dark</item>
        <item>@array/color_money_primary_dark</item>
        <item>@array/color_chart_primary_dark</item>
    </integer-array>
</resources>