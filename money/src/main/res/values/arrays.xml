<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Drawer Labels -->
    <string-array name="label_items_my">
        <item>@string/item_wallet</item>
        <item>@string/item_type</item>
    </string-array>

    <string-array name="label_items_money">
        <item>@string/item_transactions</item>
        <item>@string/item_incoming</item>
        <item>@string/item_expense</item>
    </string-array>

    <string-array name="label_items_chart">
        <item>@string/item_chart_day</item>
        <item>@string/item_chart_week</item>
        <item>@string/item_chart_month</item>
        <item>@string/item_chart_year</item>
    </string-array>

    <string-array name="label_items_others">
        <item>@string/item_settings</item>
        <item>@string/item_feedback</item>
    </string-array>
    <!--####-->

    <!--Drawer Icons-->
    <integer-array name="icon_items_my">
        <item>@drawable/ic_action_wallet</item>
        <item>@drawable/ic_action_type</item>
    </integer-array>
    <integer-array name="icon_items_money">
        <item>@drawable/ic_action_transactions</item>
        <item>@drawable/ic_action_incoming</item>
        <item>@drawable/ic_action_expense</item>
    </integer-array>
    <integer-array name="icon_items_others">
        <item>@drawable/ic_action_settings</item>
        <item>@drawable/ic_action_feedback</item>
    </integer-array>
    <integer-array name="icon_items_chart">
        <item>@drawable/ic_action_today</item>
        <item>@drawable/ic_action_week</item>
        <item>@drawable/ic_action_week</item>
        <item>@drawable/ic_action_year</item>
    </integer-array>
    <!--#########-->

    <integer-array name="drawer_labels">
        <item>@array/label_items_my</item>
        <item>@array/label_items_money</item>
        <item>@array/label_items_chart</item>
        <item>@array/label_items_others</item>
    </integer-array>
    <integer-array name="drawer_icons">
        <item>@array/icon_items_my</item>
        <item>@array/icon_items_money</item>
        <item>@array/icon_items_chart</item>
        <item>@array/icon_items_others</item>
    </integer-array>

</resources>